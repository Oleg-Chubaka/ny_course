%head
  %title Студенты
  %link(href="/css/alex_style.css" rel="stylesheet")
  %link(href="/css/bootstrap.min.scss" rel="stylesheet")
  %link(rel="stylesheet" href="/font-awesome-4.7.0/css/font-awesome.css")
  %link(rel="icon" href="/images/logo.png" type="images/png")
  %meta(name="viewport" content="width=device-width, initial-scale=1")
%body
  %h3= @student.first_name + ' ' + @student.last_name
  %ul
    - @sol_sys_obj.each do |obj|
      %li= obj.name
      %li= obj.pioneer.first_name + ' ' + obj.pioneer.last_name
      %li= obj.opening_date
      %li= obj.distance_from_sun
      %li= obj.distance_from_earth
      %li= obj.weight
      %li= obj.notes